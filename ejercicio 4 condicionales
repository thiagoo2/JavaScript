let num1 = 0;
let num2 = 0;

num1 = parseInt(prompt("Introduce primer número"));
num2 = parseInt(prompt("Introduce segundo número"));

if (num1 > num2) {
    console.log("El mayor es el primero: " + num1);
} else if (num2 > num1) {
    console.log("El mayor es el segundo: " + num2);
} else {
    console.log("Los números son iguales");
}
